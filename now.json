{
    "version": 2,
    "name": "torrent2magnet",
    "scope": "messycode",
    "builds": [
        { "src": "*.php", "use": "vercel-php@0.7.3" },
        { "src": "asset/*", "use": "@now/static" },
        { "src": "asset/uploadify/*", "use": "@now/static" },
        { "src": "*.html", "use": "@now/static" },
        { "src": "*.ico", "use": "@now/static" }
    ],
    "routes": [
        { "handle": "filesystem" },
        { "src": "/.*", "status": 404, "dest": "/404.html" }
    ],
    "alias": ["torrent2magnet.9527.eu.org"]
  }
